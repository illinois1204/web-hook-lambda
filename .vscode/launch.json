{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "local run",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--host",
                "0.0.0.0",
                "--port",
                "5001",
                "--env-file",
                ".env",
                "--reload"
            ],
            "jinja": true
            // "env": {
            //     "PYTHONPYCACHEPREFIX": "${workspaceFolder}/__pycache__"
            // }
        },
        {
            "name": "Attach Python",
            "type": "debugpy",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}